#include<stdio.h>
#include<math.h>
int main()
{
  int n1,n2,n3;
  char op;
  printf("Enter two numbers:");
  scanf("%d %d",&n1,&n2);
  printf("Enter an operation:");
  scanf("%c",&op);
  switch(op)
  {
    case '+':
     res=n1+n2;
     printf("result:%d",res);
     break;
    case '-':
     res=n1-n2;
      printf("result:%d",res);
      break;
     case '*':
      res=n1*n2;
      printf("result:%d",res);
      break;
     case '/':
      res=n1/n2;
      printf("result:%d",res);
      break;
     case '^':
      res=pow(n1,n2);
      printf("result:%d",res);
      break;
     default:
      printf("Enter correct operator");
      break;
    }
     return 0;
}

/*
 output:
 1.Enter two numbers:5 6
   Enter operation:+
   result:11
 2.Enter two numbers:6 5
   Enter operation:-
   result:1
*/  
